{
  "kind": "collectionType",
  "collectionName": "textbooks",
  "info": {
    "singularName": "textbook",
    "pluralName": "textbooks",
    "displayName": "Textbook"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "file": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "grade": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::grade.grade",
      "inversedBy": "textbooks"
    },
    "section": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::section.section",
      "inversedBy": "textbooks"
    },
    "subject": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subject.subject",
      "inversedBy": "textbooks"
    }
  }
}
