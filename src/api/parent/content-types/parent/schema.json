{
  "kind": "collectionType",
  "collectionName": "parents",
  "info": {
    "singularName": "parent",
    "pluralName": "parents",
    "displayName": "Parent"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "private": true,
      "minLength": 6
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::student.student",
      "inversedBy": "parents"
    },
    "seat_reservations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::seat-reservation.seat-reservation",
      "mappedBy": "parent"
    },
    "leave_requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::leave-request.leave-request",
      "mappedBy": "parent"
    }
  }
}
