{
  "kind": "collectionType",
  "collectionName": "art_groups",
  "info": {
    "singularName": "art-group",
    "pluralName": "art-groups",
    "displayName": "Art Group"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "code": {
      "type": "string",
      "unique": true
    },
    "art_timetable_entries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::art-timetable-entry.art-timetable-entry",
      "mappedBy": "art_groups"
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student.student",
      "mappedBy": "art_group"
    }
  }
}
