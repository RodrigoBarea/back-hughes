{
  "kind": "collectionType",
  "collectionName": "timetable_entries",
  "info": {
    "singularName": "timetable-entry",
    "pluralName": "timetable-entries",
    "displayName": "Timetable Entry"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "day": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "monday",
        "tuesday",
        "wednesday",
        "thursday",
        "friday"
      ]
    },
    "period": {
      "type": "integer"
    },
    "startTime": {
      "type": "time"
    },
    "endTime": {
      "type": "time"
    },
    "notes": {
      "type": "text"
    },
    "sections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::section.section",
      "inversedBy": "timetableEntries"
    },
    "subject": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subject.subject",
      "inversedBy": "timetableEntries"
    },
    "teacher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::teacher.teacher",
      "inversedBy": "timetableEntries"
    }
  }
}
